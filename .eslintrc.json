{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "react/react-in-jsx-scope": "off",
    "import/prefer-default-export": "off"
  }
  //   "plugins": ["react"],
  //   "rules": {
  //     "prettier/prettier": [
  //       "error",
  //       { "endOfLine": "auto" },
  //       { "usePrettierrc": true }
  //     ]
  //   }
}
